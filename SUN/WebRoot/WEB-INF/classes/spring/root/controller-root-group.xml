<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!--后台管理登录-->
	<bean id="adminGroupController" class="com.sun.app.common.BaseController">
		<property name="processMap">
			<map>
				<entry key="default">
					<bean class="com.sun.app.process.admin.group.ListProcess">
						<property name="successView" value="admin/group/list" />
					</bean>
				</entry>
				<entry key="list">
					<bean class="com.sun.app.process.admin.group.ListProcess">
						<property name="successView" value="admin/group/list" />
					</bean>
				</entry>
				<entry key="addview">
					<bean class="com.sun.app.process.admin.group.AddViewProcess">
						<property name="successView" value="admin/group/add_view" />
					</bean>
				</entry>
				<entry key="adddialog">
					<bean class="com.sun.app.process.admin.group.AddViewProcess">
						<property name="successView" value="admin/group/add_dialog" />
					</bean>
				</entry>
				<entry key="add">
					<bean class="com.sun.app.process.admin.group.AddProcess">
						<property name="successView" value="jsonView" />
					</bean>
				</entry>	
				<entry key="editview">
					<bean class="com.sun.app.process.admin.group.EditViewProcess">
						<property name="successView" value="admin/group/edit_view" />
					</bean>
				</entry>	
				<entry key="edit">
					<bean class="com.sun.app.process.admin.group.EditProcess">
						<property name="successView" value="jsonView" />
					</bean>
				</entry>	
				<entry key="delete">
					<bean class="com.sun.app.process.admin.group.DeleteProcess">
						<property name="successView" value="jsonView" />
					</bean>
				</entry>					
			</map>
		</property>
	</bean>
</beans>